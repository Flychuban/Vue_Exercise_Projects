{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "jest"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.5.1",
    "lodash": "^4.17.21",
    "nuxt": "^3.12.3",
    "pinia": "^2.1.7",
    "vue": "latest",
    "vue-tsc": "^2.0.26"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^3.0.2",
    "@pinia/testing": "^0.1.3",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@vue/compiler-sfc": "^3.4.31",
    "@vue/test-utils": "^2.4.0-alpha.2",
    "@vue/vue3-jest": "^29.2.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.3",
    "typescript": "^5.5.3",
    "vite-plugin-checker": "^0.7.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jest-environment-jsdom",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue",
      "tsx",
      "jsx"
    ],
    "transform": {
      ".*\\.(vue)$": "@vue/vue3-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": ["**/tests/**/*.spec.[jt]s?(x)", "**/__tests__/*.[jt]s?(x)"]
  }
}
